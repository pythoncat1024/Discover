<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/account_text" />

        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/bg_device_lock_open" />

        </shape>
    </item>

    <item android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="@color/btn_select_conversation_disable" />

        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/bg_app_help" />
        </shape>
    </item>
</selector>