<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_20" />
            <stroke android:width="@dimen/dp_2" android:color="@color/bg_device_lock_close" />
            <solid android:color="@color/account_text" />
        </shape>
    </item>

    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_20" />
            <stroke android:width="@dimen/dp_2" android:color="@color/bg_device_lock_close" />
            <solid android:color="@color/account_text" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_20" />
            <stroke android:width="@dimen/dp_2" android:color="@color/banner_bg" />
            <solid android:color="@color/collect_gray" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_20" />
            <stroke android:width="@dimen/dp_2" android:color="@color/banner_bg" />
            <solid android:color="@color/btn_select_conversation_disable" />
        </shape>
    </item>
</selector>